# Camera
allow vendor_hal_perf_default hal_camera_default:dir r_dir_perms;
allow vendor_hal_perf_default hal_camera_default:file r_file_perms;

# Displayfeature
allow vendor_hal_perf_default vendor_hal_displayfeature_xiaomi_default:dir search;
allow vendor_hal_perf_default vendor_hal_displayfeature_xiaomi_default:file read;

# Memory
allow vendor_hal_perf vendor_sysfs_memory:dir r_dir_perms;
allow vendor_hal_perf vendor_sysfs_memory:file rw_file_perms;

# Perf
allow vendor_hal_perf_default surfaceflinger:dir { open read search };
allow vendor_hal_perf_default surfaceflinger:file { open read };
allow vendor_hal_perf_default surfaceflinger:process setsched;
allow vendor_hal_perf_default hal_graphics_composer_default:dir r_dir_perms;
allow vendor_hal_perf_default hal_graphics_composer_default:file r_file_perms;

# Power
allow vendor_hal_perf_default hal_power_default:dir r_dir_perms;
allow vendor_hal_perf_default hal_power_default:file r_file_perms;

# Thermal
allow vendor_hal_perf_default thermal_data_file:dir { add_name read remove_name search watch write };
allow vendor_hal_perf_default thermal_data_file:file { create getattr open read rename setattr unlink write };
allow vendor_hal_perf_default mi_thermald:dir r_dir_perms;
allow vendor_hal_perf_default mi_thermald:file r_file_perms;
allow vendor_hal_perf_default sysfs_thermal:file rw_file_perms;

allow vendor_hal_perf_default kernel:process setsched;
allow vendor_hal_perf_default sysfs_transparent_hugepage:file { open write };
allow vendor_hal_perf_default vendor_fingerprint_device:dir r_dir_perms;
allow vendor_hal_perf_default vendor_fingerprint_device:dir search;
allow vendor_hal_perf_default vendor_fingerprint_device:file r_file_perms;
allow vendor_hal_perf_default vendor_fingerprint_device:file read;
allow vendor_hal_perf_default hal_graphics_composer_default:dir search;
r_dir_file(vendor_hal_perf_default, domain)
